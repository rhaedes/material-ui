    "json-loader": "^0.5.4",
    "marked": "^0.3.5",
    "node-sass": "^3.7.0",
    "raw-loader": "^0.5.1",
    "react-addons-perf": "^15.0.1",
    "react-docgen": "^2.4.0",
    "react-hot-loader": "^1.2.8",
    "react-motion": "^0.4.2",
    "react-native": "^0.17.0",
    "react-router": "^2.0.0",
    "react-swipeable-views": "^0.5.0",
    "recast": "^0.11.4",
